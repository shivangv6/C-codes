#include <iostream>
#include<string>

using namespace std;

int main(){
  int year;
  std::cout << "type the year/n";
  std::cin >> year;
  std::string a = to_string(year);
  if(a.length() == 4){
    if(year%4 ==0){
      if(year%100 !=0){
        std::cout << "It is a leap year\n";
      }
      else if(year%400==0){
        std::cout << "It is a leap year\n";
      }
      else{
        std::cout << "It is not a leap year\n";
      }
    }
  }
}
